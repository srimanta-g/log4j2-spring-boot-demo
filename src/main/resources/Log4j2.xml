<?xml version="1.0" encoding="UTF-8"?>
<configuration info="info">
	<Appenders>
		<Console  name="LogToConsole" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss} [%p] %c{1}:%L - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="LogToConsole"/>
		</Root>
	</Loggers>
</configuration>